{
  "name": "ascenso",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --testPathPattern ./src/**/*.ts",
    "test:e2e": "npm run build:e2e && mkdir -p .test_artifacts/e2e && nightwatch",
    "lint": "vue-cli-service lint",
    "build:e2e": "tsc -outDir .e2e_js $(find e2e -name '*.ts')",
    "test:e2e:auto": "start-server-and-test serve 8080 test:e2e:xvfb",
    "test:e2e:xvfb": "xvfb-run -s ':99 -ac -shmem -screen 0 1600x1200x16' npm run test:e2e",
    "test:firestore": "firebase emulators:exec --only firestore \"vue-cli-service test:unit --testPathPattern ./firestore.test.ts\"",
    "test:unit:only": "vue-cli-service test:unit --testPathPattern"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "acorn": "^7.1.0",
    "core-js": "^3.4.4",
    "firebase": "^6.6.2",
    "firebaseui": "^4.0.0",
    "humanize-duration": "^3.20.1",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-i18n": "^8.13.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.7",
    "vue-the-mask": "^0.11.1",
    "vuefire": "^2.1.0",
    "vuetify": "^2.2.4"
  },
  "devDependencies": {
    "@firebase/testing": "^0.12.0",
    "@mdi/font": "^4.4.95",
    "@types/humanize-duration": "^3.18.0",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.138",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "chromedriver": "^76.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^6.0.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "firebase-tools": "^7.2.4",
    "flush-promises": "^1.0.2",
    "jest": "^24.8.0",
    "nightwatch": "^1.2.1",
    "release-it": "^12.4.3",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "start-server-and-test": "^1.10.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3"
  }
}
