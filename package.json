{
  "name": "ascenso",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:e2e": "tsc -outDir .e2e_js $(find e2e -name '*.ts')",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --testPathPattern ./src/**/*.ts",
    "test:unit:only": "vue-cli-service test:unit --testPathPattern",
    "test:firestore": "firebase emulators:exec --only firestore \"vue-cli-service test:unit --testPathPattern ./firestore.test.ts\"",
    "test:e2e": "npm run build:e2e && mkdir -p .test_artifacts/e2e && nightwatch",
    "test:e2e:xvfb": "xvfb-run -s ':99 -ac -shmem -screen 0 1600x1200x16' npm run test:e2e",
    "test:e2e:auto": "start-server-and-test serve 8080 test:e2e:xvfb"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "acorn": "^7.1.0",
    "core-js": "^2.6.11",
    "firebase": "^6.6.2",
    "firebaseui": "^4.3.0",
    "humanize-duration": "^3.21.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-i18n": "^8.15.1",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-the-mask": "^0.11.1",
    "vuefire": "^2.2.0",
    "vuetify": "^2.1.13",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@firebase/testing": "^0.12.3",
    "@mdi/font": "^4.7.95",
    "@types/humanize-duration": "^3.18.0",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "axios": "^0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "chromedriver": "^76.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "eslint-plugin-vuetify": "^1.0.0-beta.5",
    "firebase-tools": "^7.9.0",
    "flush-promises": "^1.0.2",
    "jest": "^24.9.0",
    "nightwatch": "^1.3.2",
    "release-it": "^12.4.3",
    "sass": "^1.23.7",
    "sass-loader": "^7.3.1",
    "start-server-and-test": "^1.10.6",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "plugins": [
      "vuetify"
    ],
    "rules": {
      "no-console": "off",
      "vuetify/no-deprecated-classes": "error",
      "vuetify/grid-unknown-attributes": "error",
      "vuetify/no-legacy-grid": "error"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
